{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@icode-tfs-ngrx-demo/pacsurl": [
        "lib/pacsurl/pacsurl-feature/src/index.ts"
      ],
      "@icode-tfs-ngrx-demo/pacsurl-domain": [
        "libs/pacsurl/domain/src/index.ts"
      ],
      "@icode-tfs-ngrx-demo/pacsurl-feature": [
        "libs/pacsurl/feature/src/index.ts"
      ],
      "@icode-tfs-ngrx-demo/pacsurl-ui": ["libs/pacsurl/ui/src/index.ts"],
      "@icode-tfs-ngrx-demo/ui-common": ["libs/shared/ui-common/src/index.ts"],
      "@icode-tfs-ngrx-demo/user-domain": ["libs/users/domain/src/index.ts"],
      "@icode-tfs-ngrx-demo/user-features": [
        "libs/users/features/src/index.ts"
      ],
      "@icode-tfs-ngrx-demo/user-ui": ["libs/users/ui/src/index.ts"],
      "@icode-tfs-ngrx-demo/util-authentication": [
        "libs/shared/util-authentication/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
